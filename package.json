{
  "name": "lanternleaf",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.28.2",
  "scripts": {
    "dev": "pnpm tauri:dev",
    "build": "pnpm tauri:build",
    "ui:dev": "pnpm --dir ui run dev",
    "ui:build": "pnpm --dir ui run build",
    "ui:check": "pnpm --dir ui run check",
    "ui:lint": "pnpm --dir ui run lint",
    "types:generate": "cargo run --manifest-path src-tauri/Cargo.toml --bin export_ts_bindings",
    "types:check": "pnpm run types:generate && git diff --exit-code -- ui/src/generated",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "tauri:soak": "pnpm --dir ui run test:e2e:tauri:soak"
  },
  "devDependencies": {
    "@tauri-apps/cli": "2.10.0"
  }
}
